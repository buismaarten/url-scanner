{
    "name": "buismaarten/url-scanner",
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/buismaarten/url-scanner/issues",
        "source": "https://github.com/buismaarten/url-scanner"
    },
    "authors": [
        {
            "name": "Maarten Buis"
        }
    ],
    "require": {
        "php": "^8.2",
        "league/uri": "^7.3",
        "league/uri-interfaces": "^7.3",
        "symfony/dom-crawler": "^7.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Buismaarten\\UrlScanner\\": "src/"
        }
    },
    "scripts": {
        "analyse": "./vendor/bin/phpstan analyse -c phpstan.neon",
        "test": "./vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
